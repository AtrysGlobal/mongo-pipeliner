{
  "name": "mongo-pipeliner",
  "version": "0.1.2",
  "description": "A practical and userful set of tools to help you build and test MongoDB aggregation pipelines.",
  "keywords": [
    "mongodb",
    "mongoose",
    "aggregation",
    "pipeline",
    "atrys",
    "atrysglobal",
    "atryshealth"
  ],
  "author": "Atrys Health.",
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register --esm '__test__/**/*.spec.ts' '__test__/**/*.spec.ts' --timeout 60000 --exit",
    "build": "tsc",
    "lint": "eslint",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint && npm test",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtrysGlobal/mongo-pipeliner.git"
  },
  "pre-push": [
    "preversion"
  ],
  "type": "module",
  "bugs": {
    "url": "https://github.com/AtrysGlobal/mongo-pipeliner/issues"
  },
  "homepage": "https://github.com/AtrysGlobal/mongo-pipeliner#readme",
  "dependencies": {
    "mongoose": "^7.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^9.1.6",
    "pre-push": "^0.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
